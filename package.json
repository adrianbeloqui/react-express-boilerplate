{
  "name": "react_and_express_boilerplate",
  "version": "0.1.0",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.3",
    "express-formidable": "^1.2.0",
    "fibers": "^4.0.2",
    "helmet": "^3.21.2",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.0",
    "regenerator-runtime": "^0.13.3",
    "sass": "^1.25.0",
    "serve": "^11.3.0",
    "typescript": "^3.7.5",
    "winston": "^3.2.1"
  },
  "scripts": {
    "start:react": "react-scripts start",
    "start:dev": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && nodemon --exec babel-node server.js",
    "build:react": "react-scripts build",
    "build:express": "export NODE_ENV='production' && webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV='production' && rm -rf build && react-scripts build && webpack --config webpack.config.js",
    "start:prod": "export NODE_ENV='production' && nodejs build/app.bundle.js",
    "build:analyze-react": "source-map-explorer 'build/static/js/*.js' --html react_tree.html",
    "build:analyze-express": "source-map-explorer build/app.bundle.js build/app.bundle.js.map --html express_tree.html",
    "test:react": "react-scripts test",
    "eject:react": "react-scripts eject",
    "format": "prettier --write --single-quote --no-semi --print-width=120 --tab-width=2 \"**/**/*.{js,jsx} \"",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "lint:fix": "eslint '**/*.{js,jsx}' --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "core-js": "^3.6.4",
    "css-loader": "^3.4.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.18.3",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "source-map-explorer": "^2.2.2",
    "style-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "sourceType": "unambiguous",
    "plugins": [
      "@babel/transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "description": "React + ExpressJS project that creates stories in JIRA and other platforms.",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
